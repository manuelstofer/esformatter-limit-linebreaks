{
  "preset": "default",
  "plugins": [
    "esformatter-semicolons",
    "esformatter-quotes",
    "esformatter-var-each",
    "esformatter-align",
    "esformatter-limit-linebreaks",
    "esformatter-eol-last"
  ],
  "quotes": {
    "type": "single",
    "avoidEscape": true
  },

  "LimitLineBreaks": 2
}
